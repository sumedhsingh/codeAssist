{
  "name": "codeinsight-vscode",
  "displayName": "CodeInsight",
  "description": "Advanced code analysis using Groq API",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [
      "onCommand:extension.runCodeInsight"
  ],
  "main": "./extension.js",
  "contributes": {
      "commands": [{
          "command": "extension.runCodeInsight",
          "title": "Run CodeInsight Analysis"
      }],
      "configuration": {
          "title": "CodeInsight",
          "properties": {
              "codeinsight.pythonPath": {
                  "type": "string",
                  "default": "python",
                  "description": "Path to Python interpreter"
              }
          }
      }
  },
  "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "node ./test/runTest.js"
  },
  "devDependencies": {
      "@types/vscode": "^1.60.0",
      "@types/node": "14.x",
      "eslint": "^7.27.0",
      "typescript": "^4.3.2"
  }
}